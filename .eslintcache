} type=\"number\" onChange={(e) => setCash(e.target.value)}/>}\r\n            {cash && <button onClick={cashReturn} className=\"cashBtn\">Return Amount</button>}\r\n        </form>\r\n        {diff && <h1>Return Amount: ${diff}</h1>}\r\n        {diff && <button onClick={showFrequency} className=\"cashBtn\">Show Currency Notes</button>}\r\n        {diff && <div className=\"cashOutput\">\r\n        {Object.entries(freq).map(([key, value]) => {\r\n            return <h2 className=\"cashNotes\"><span className=\"notes\">{value}</span> notes of <span className=\"notes\">${key}</span></h2>\r\n        })} \r\n        </div>}\r\n        <button onClick={nextCal} className=\"cashBtn done\">Done</button>\r\n        </div>\r\n    )\r\n}\r\n","D:\\JavaScript\\learner1\\smallapps\\src\\Components\\Practice.js",[],["60","61"],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\Footer.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\Header.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\LuckyBirthday.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\HeroSection.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\Heading.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\ProfitLoss.js",[],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\GroceryList.js",["62","63","64"],"D:\\JavaScript\\learner1\\smallapps\\src\\Components\\InputGrocery.js",[],{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","severity":1,"message":"70","line":33,"column":34,"nodeType":"71","messageId":"72","endLine":33,"endColumn":36},{"ruleId":"65","replacedBy":"73"},{"ruleId":"67","replacedBy":"74"},{"ruleId":"69","severity":1,"message":"70","line":33,"column":33,"nodeType":"71","messageId":"72","endLine":33,"endColumn":35},{"ruleId":"75","severity":1,"message":"76","line":50,"column":13,"nodeType":"77","endLine":50,"endColumn":36},{"ruleId":"75","severity":1,"message":"76","line":59,"column":13,"nodeType":"77","